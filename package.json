  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@material-ui/core": "^3.2.0",
    "@material-ui/icons": "^3.0.1",
    "@reach/router": "^1.2.1",
    "babel-core": "6.26.0",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "bugsnag-js": "^4.7.3",
    "bugsnag-react": "^1.1.1",
    "classnames": "^2.2.6",
    "color": "^3.1.0",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "focus-juggler": "git://github.com/skilltype/focus-juggler.git",
    "indefinite": "^2.1.1",
    "is-react": "^1.3.3",
    "jss": "^9.8.7",
    "jss-preset-default": "^4.5.0",
    "lodash.merge": "^4.6.1",
    "mapbox-gl": "^1.0.0",
    "moment": "^2.24.0",
    "object-assign": "4.1.1",
    "papaparse": "^4.6.3",
    "pluralize": "^7.0.0",
    "promise": "8.0.1",
    "prop-types": "^15.6.2",
    "qs": "^6.7.0",
    "raf": "3.4.0",
    "react": "^16.6.1",
    "react-autosuggest": "^9.4.2",
    "react-contenteditable": "^2.1.5",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.6.1",
    "react-isolated-scroll": "^0.1.1",
    "react-jss": "^8.6.1",
    "react-mapbox-gl": "^4.4.0",
    "react-modal": "^3.8.1",
    "react-portal": "^4.2.0",
    "react-responsive": "^5.0.0",
    "react-scrolllock": "^3.0.2",
    "react-select": "^2.1.1",
    "react-svg-loader": "^2.1.0",
    "react-text-mask": "^5.4.3",
    "scroll-into-view-if-needed": "^2.2.20",
    "shallowequal": "^1.1.0",
    "tabbable": "^3.1.2"
  },
  "scripts": {
    "start": "NPM_DEPLOY_TARGET=USER_APP yarn build && NPM_BUILD_TARGET=USER_APP node scripts/start.js",
    "start:login": "NPM_BUILD_TARGET=LOGIN_APP PUBLIC_URL=/login node scripts/start.js",
    "build": "bash scripts/build.sh",
    "test": "node scripts/test.js",
    "flow": "flow",
    "prepush": "yarn sync-versions && yarn lint && yarn test",
    "styleguide": "styleguidist server --config ./config/styleguide.config.js",
    "styleguide:build": "styleguidist build --config ./config/styleguide.config.js",
    "storybook": "start-storybook -h 0.0.0.0 -p 6006 -s ./public",
    "storybook:build": "build-storybook -o build  -s ./public && node scripts/sb-publish.js Skilltype",
    "lint": "eslint . --cache && echo \"eslint: no lint errors\"",
    "sync-versions": "node scripts/sync-versions.js",
    "import-data": "node scripts/import-data.js"

  },
  "jest": {
    "projects": [
      {
        "displayName": "lint",
        "runner": "jest-runner-eslint",
        "testMatch": [
          "<rootDir>/**/*.js"
        ],
        "testPathIgnorePatterns": [
          "/node_modules/"
        ]
      },
      {
        "displayName": "test",
        "collectCoverageFrom": [
          "**/*.{js,jsx,mjs}"
        ],
        "resolver": "jest-pnp-resolver",
        "setupFiles": [
          "<rootDir>/config/polyfills.js"
        ],
        "testMatch": [
          "<rootDir>/**/*.test.{js,jsx,mjs}"
        ],
        "testEnvironment": "jsdom",
        "testURL": "http://localhost",
        "transform": {
          "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
          "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
          "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
        ],
        "moduleFileExtensions": [
          "web.js",
          "js",
          "json",
          "web.jsx",
          "jsx",
          "node"
        ]
      },
      {
        "displayName": "checkversion",
        "runner": "./tests/checkversion.runner.js",
        "testMatch": [
          "<rootDir>/**/package.json"
        ],
        "testPathIgnorePatterns": [
          "/node_modules/"
        ]
      }
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "babel-plugin-transform-builtin-extend",
        {
          "globals": [
            "Error"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.10",
    "@storybook/addon-links": "^3.4.10",
    "@storybook/addon-options": "^3.4.10",
    "@storybook/addon-storyshots": "^3.4.11",
    "@storybook/addons": "^3.4.10",
    "@storybook/react": "^3.4.10",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.4",
    "babel-eslint": "^8.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "copy-webpack-plugin": "^4.5.4",
    "create-jest-runner": "^0.4.1",
    "css-loader": "^2.1.1",
    "csv-loader": "^3.0.2",
    "csvtojson": "^2.0.8",
    "eslint": "4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "https://github.com/pzhine/eslint-plugin-import.git#release-temp",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "flow-bin": "^0.79.1",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "^1.0.1",
    "jest-resolve": "^23.6.0",
    "jest-runner-eslint": "^0.7.0",
    "lorem-ipsum": "^1.0.6",
    "prettier": "1.13.5",
    "prettier-eslint": "^8.8.1",
    "react-styleguidist": "^7.3.4",
    "react-test-renderer": "16.6.3",
    "resolve": "1.6.0",
    "semver-diff": "^2.1.0",
    "serve": "^10.1.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  }
}
